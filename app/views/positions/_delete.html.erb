<div class="modal-header">
    <h4 class="modal-title" id="myModalLabel"><%= t('positions.delete.ask_to_delete') %></h4>
    <button type="button" class="btn-close" data-bs-dismiss="modal" aria-hidden="true"></button>
</div>

<div class="modal-footer">	
    <%= link_to  sub_delete_positions_path(ids: params[:data][:ids]),remote: true, id: "btnModalDelete", class: "btn btn-danger btnDeleteDepart"  do %>
        <i class="uil uil-minus-circle iconDeleteDepart"></i><%= t('positions.delete.delete') %>
    <% end %>
    <button type="button" class="btn btn-light" data-bs-dismiss="modal"><%= t('positions.delete.close_up') %></button>
</div>